{
  "expo": {
    "name": "Solution Center",
    "slug": "solution-center",
    "sdkVersion":"39.0.0",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "notification": {
      "icon": "./assets/notification.png",
      "color": "#000D2B",
      "androidMode": "default"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "br.com.ylanbenoliel.solutioncenter",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "É necessário para salvar foto de perfil."
      }
    },
    "android": {
      "package": "br.com.solutioncenter.solutioncenter",
      "versionCode": 1,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "useNextNotificationsApi": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
